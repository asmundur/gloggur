# Core configuration (matches README defaults)
GLOGGUR_EMBEDDING_PROVIDER=gemini
GLOGGUR_LOCAL_MODEL=microsoft/codebert-base
GLOGGUR_OPENAI_MODEL=text-embedding-3-large
GLOGGUR_GEMINI_MODEL=gemini-embedding-001
GLOGGUR_CACHE_DIR=.gloggur-cache

# API keys (set one or more as needed)
GLOGGUR_GEMINI_API_KEY=
GEMINI_API_KEY=
GOOGLE_API_KEY=
OPENAI_API_KEY=

# Verification logging (optional)
GLOGGUR_LOG_LEVEL=INFO
GLOGGUR_LOG_FILE=
GLOGGUR_TRACE_ID=
GLOGGUR_DEBUG_LOGS=

# Verification performance thresholds (optional)
GLOGGUR_PERF_INDEX_MAX_MS=30000
GLOGGUR_PERF_INCREMENTAL_MAX_MS=10000
GLOGGUR_PERF_SEARCH_MAX_MS=5000
